{
    "name": "country-data-list",
    "version": "1.2.0",
    "description": "Data about countries - like their ISO codes and currencies",
    "homepage": "https://github.com/Sonatrix/country-list",
    "bugs": {
        "url": "https://github.com/Sonatrix/country-list/issues"
    },
    "main": "dist/index.js",
    "lint-staged": {
        "*.js": [
            "eslint --fix",
            "prettier --config .prettierrc --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "scripts": {
        "build": "rm -r dist/* && babel src -d dist",
        "test": "mocha",
        "lint": "eslint src test",
        "prepare": "npm run lint && npm run test && npm run build",
        "eslint-check": "eslint --print-config .eslintrc | eslint-config-prettier-check",
        "prettify": "./node_modules/.bin/prettier --config .prettierrc --write '*/**/*.{js,jsx}'"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Sonatrix/country-list.git"
    },
    "keywords": [
        "data",
        "country",
        "es6",
        "es2015",
        "babel",
        "mocha",
        "chai",
        "eslint",
        "travis",
        "travis-ci"
    ],
    "author": "Sonatrix",
    "license": "MIT",
    "dependencies": {
        "currency-symbol-map": "~4"
    },
    "devDependencies": {
        "@babel/cli": "^7.8.4",
        "@babel/core": "^7.9.0",
        "@babel/preset-env": "^7.9.0",
        "@babel/register": "^7.9.0",
        "babel-eslint": "10.1.0",
        "chai": "4.2.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.1",
        "eslint-plugin-import": "^2.20.1",
        "eslint-plugin-prettier": "^3.1.2",
        "husky": "^4.2.3",
        "lint-staged": "^10.0.9",
        "lodash": ">=4.17.15",
        "mixin-deep": ">=2.0.1",
        "mocha": "^7.1.1",
        "prettier": "^2.0.2",
        "set-value": ">=3.0.1"
    }
}
