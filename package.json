{
    "name": "country-data-list",
    "version": "1.0.0",
    "description": "Data about countries - like their ISO codes and currencies",
    "homepage": "https://github.com/Sonatrix/country-list",
    "bugs": {
        "url": "https://github.com/Sonatrix/country-list/issues"
    },
    "main": "dist/country-data-list.js",
    "lint-staged": {
        "*/**/*.{js}": [
            "eslint --fix",
            "prettier --config .prettierrc --write"
        ]
    },
    "scripts": {
        "precommit": "lint-staged",
        "build": "webpack --mode=production",
        "test": "mocha --require babel-register",
        "lint": "eslint src test",
        "prepare": "npm run lint && npm run test && npm run build",
        "eslint-check": "eslint --print-config .eslintrc | eslint-config-prettier-check",
        "prettify": "./node_modules/.bin/prettier --config .prettierrc --write '*/**/*.{js,jsx}'"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Sonatrix/country-list.git"
    },
    "keywords": [
        "data",
        "country",
        "es6",
        "es2015",
        "babel",
        "webpack4",
        "mocha",
        "chai",
        "eslint",
        "travis",
        "travis-ci"
    ],
    "author": "Sonatrix",
    "license": "MIT",
    "dependencies": {
        "currency-symbol-map": "~4",
        "underscore": ">1.4.4"
    },
    "devDependencies": {
        "babel-core": "6.26.0",
        "babel-eslint": "8.2.3",
        "babel-loader": "7.1.4",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-preset-env": "1.6.1",
        "babel-register": "6.26.0",
        "chai": "4.1.2",
        "eslint": "4.19.1",
        "eslint-config-airbnb": "16.1.0",
        "eslint-config-prettier": "^2.9.0",
        "eslint-plugin-import": "^2.7.0",
        "eslint-plugin-jsx-a11y": "^6.0.2",
        "eslint-plugin-prettier": "^2.6.0",
        "eslint-plugin-react": "^7.4.0",
        "husky": "^0.14.3",
        "lint-staged": "^7.0.4",
        "mocha": "^5.1.1",
        "prettier": "^1.12.0",
        "webpack": "4.6.0",
        "webpack-cli": "^2.0.15"
    }
}
